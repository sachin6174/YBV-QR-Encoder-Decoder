{
    "manifest_version": 3,
    "name": "YBV QR Encoder Decoder",
    "description": "Read QR for debuging code.",
    "version": "1.0.0",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}